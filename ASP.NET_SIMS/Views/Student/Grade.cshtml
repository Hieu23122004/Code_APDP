@{
    ViewData["Title"] = "Bảng điểm của tôi";
}
<div class="mb-3">
    <a asp-action="Grade" asp-controller="Student" class="btn btn-primary">Xem Điểm số</a>
    <a asp-action="MyCourse" asp-controller="Student" class="btn btn-secondary">Xem Khóa Học</a>
  
    <a asp-action="Login" asp-controller="Account" class="btn btn-info">Logout</a>
</div>

<h2 class="mb-4">Bảng Điểm của tôi</h2>
<div class="container mt-4">
        <table class="table table-bordered table-hover table-striped">
            <thead class="table-dark text-center">
                <tr>
                    <th>ID</th>
                    <th>Student ID</th>
                    <th>Họ và tên</th>
                    <th>Course</th>
                    <th>Diểm</th>
                    <th>Trạng thái</th>
                </tr>
            </thead>
            
        </table>
</div>
